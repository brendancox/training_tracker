<h1>Add Workout Template</h1>

<div class="row">
  <div class="col-md-8">

    <%= form_for(@workout) do |f| %>

      <%= f.label "Name" %>
      <%= f.text_field :name, value: @template_workout[:name] %><br>

      <%= render 'layouts/workout_form', f: f %>

        <tr>
          <td></td>
          <td><%= f.submit "Save Template"%></td>
        </tr>

      </table>

    <% end %>

  </div>
</div>